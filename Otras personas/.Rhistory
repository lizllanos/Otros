mean22=function(a,na.rm=T){
na.x=sum(is.na(a))/length(a)
if(na.x>=0.5){
x=NA
}else{x=mean(a,na.rm=T)}
return(x)
}
sum22=function(a,na.rm=T){
na.x=sum(is.na(a))/length(a)
if(na.x>=0.2){
x=NA
}else{x=sum(a,na.rm=T)}
return(x)
}
